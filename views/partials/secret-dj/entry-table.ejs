<% if (season.entries.length) { %>
<table>
	<tr>
		<th>recipient</th>
		<% if (season.ruleCount > 1) { %>
		<% for (let i = 0; i < season.ruleCount; i++) { %>
		<th>
			rule
			<%=i + 1%>
		</th>
		<% } %>
		<% } else { %>
		<th>rule</th>
		<% } %>
		<th>entry page</th>
	</tr>
	<% for (const entry of season.entries) { %>
	<% const link = `/secret-dj/games/${season.id}/entries/${entry.id}`%>
	<a href="<%=link%>">
		<tr>
			<td><a href="/secret-dj/djs/<%= entry.recipient.id %>"><%= entry.recipient.name %></a></td>
			<% for (const rule of entry.rules) { %>
			<td><%=rule.text%></td>
			<% } %>
			<td><a href="<%=link%>">view</a></td>
		</tr>
	</a>
	<% } %>
</table>
<% } else { %>
no one has signed up for this season of secret dj yet
<% } %>
