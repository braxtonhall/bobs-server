<table>
	<tr>
		<th>recipient</th>
		<th>playlist</th>
	</tr>
	<% for (const entry of season.entries) { %>
	<% const link = `/secret-dj/games/${season.id}/entries/${entry.id}`%>
	<tr>
		<td><a href="<%=link%>"><%= entry.recipient.name %></a></td>
		<td><a href="<%= entry.submissionUrl %>" target="_blank">link</a></td>
	</tr>
	<% } %>
</table>
